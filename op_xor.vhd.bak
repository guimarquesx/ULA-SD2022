-- Objetivo: Criar um componente que realiza a operação lógica XOR.

-- Declaração de bibliotecas:
library ieee;
use ieee.std_logic_1164.all;

ENTITY op_xor IS
	PORT (
			a, b 	: IN STD_LOGIC_VECTOR(3 DOWNTO 0);
			s		: OUT STD_LOGIC_VECTOR(3 DOWNTO 0));
			
END op_xor;

ARCHITECTURE structural OF op_xor IS

BEGIN

s <= a XOR b;

END structural;